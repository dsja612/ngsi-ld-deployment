						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"@context\": \"http://orion:1026/ngsi-ld/v1/jsonldContexts/0d9f09fe-c18f-11ee-95f2-0242ac1b0006\",\r\n  \"id\": \"urn:ngsi-ld:PatrolRobot:001\",\r\n  \"type\": \"PatrolRobot\",\r\n  \"names\": {\r\n    \"type\": \"Property\",\r\n    \"value\": []\r\n  },\r\n  \"timestamp\": {\r\n    \"type\": \"Property\",\r\n    \"value\": \"\"\r\n  },\r\n  \"fileName\": {\r\n    \"type\": \"Property\",\r\n    \"value\": \"\"\r\n  },\r\n  \"imagePath\": {\r\n    \"type\": \"Property\",\r\n    \"value\": \"\"\r\n  },\r\n  \"confidence\": {\r\n    \"type\": \"Property\",\r\n    \"value\": 0\r\n  },\r\n  \"humidity\": {\r\n    \"type\": \"Property\",\r\n    \"value\": 0\r\n  },\r\n  \"pressure\": {\r\n    \"type\": \"Property\",\r\n    \"value\": 0\r\n  },\r\n  \"compass\": {\r\n    \"type\": \"Property\",\r\n    \"value\": { \r\n      \"x\": 0,\r\n      \"y\": 0,\r\n      \"z\": 0\r\n    }\r\n  },\r\n  \"gyroscope\": {\r\n    \"type\": \"Property\",\r\n    \"value\": { \r\n      \"x\": 0,\r\n      \"y\": 0,\r\n      \"z\": 0\r\n    }\r\n  },\r\n  \"accelerometer\": {\r\n    \"type\": \"Property\",\r\n    \"value\": { \r\n      \"x\": 0,\r\n      \"y\": 0,\r\n      \"z\": 0\r\n    }\r\n  },\r\n  \"temperatureAlert\": {\r\n    \"type\": \"Property\",\r\n    \"value\": false\r\n  }\r\n}\r\n",
