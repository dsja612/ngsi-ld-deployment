		{
			"name": "IOT Agent",
			"item": [
				{
					"name": "PatrolRobot example",
					"item": [
						{
							"name": "Check if the IOT Agent is running",
							"request": {
								"method": "GET",
								"header": [],
								"url": "{{iot-agent}}/iot/about"
							},
							"response": []
						},
						{
							"name": "Provisioning the PatrolRobot to the IOT agent",
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "fiware-service",
										"value": "openiot",
										"type": "text"
									},
									{
										"key": "fiware-servicepath",
										"value": "/",
										"type": "text"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\r\n  \"devices\": [\r\n    {\r\n      \"device_id\": \"PatrolRobot:010\",\r\n      \"entity_name\": \"urn:ngsi-ld:PatrolRobot:010\",\r\n      \"entity_type\": \"PatrolRobot\",\r\n      \"transport\":   \"MQTT\",\r\n      \"attributes\": [\r\n        {\r\n          \"object_id\": \"n\",\r\n          \"name\": \"names\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"t\",\r\n          \"name\": \"timestamp\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"f\",\r\n          \"name\": \"fileName\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"ip\",\r\n          \"name\": \"imagePath\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"c\",\r\n          \"name\": \"confidence\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"h\",\r\n          \"name\": \"humidity\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"p\",\r\n          \"name\": \"pressure\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"cp\",\r\n          \"name\": \"compass\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"g\",\r\n          \"name\": \"gyroscope\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"a\",\r\n          \"name\": \"accelerometer\",\r\n          \"type\": \"Property\"\r\n        },\r\n        {\r\n          \"object_id\": \"ta\",\r\n          \"name\": \"temperatureAlert\",\r\n          \"type\": \"Property\"\r\n        }\r\n      ],\r\n      \"commands\": []\r\n    }\r\n  ]\r\n}\r\n",
									"options": {
										"raw": {
											"language": "json"
										}
									}
								},
								"url": "{{iot-agent}}/iot/devices"
							},
							"response": []
						},
						{
							"name": "Get all Entities with type PatrolRobot (Context file provided) Copy",
							"request": {
								"method": "GET",
								"header": [
									{
										"key": "fiware-service",
										"value": "openiot",
										"type": "text",
										"disabled": true
									},
									{
										"key": "fiware-servicepath",
										"value": "/",
										"type": "text",
										"disabled": true
									},
									{
										"key": "link",
										"value": "<http://34.124.243.51:1026/ngsi-ld/v1/jsonldContexts/e5a9f570-c18f-11ee-95f2-0242ac1b0006>;rel=\"http://www.w3.org/ns/json-ld#context\"; type=\"application/ld+json\"",
										"type": "text"
									}
								],
								"url": {
									"raw": "{{context-broker}}/ngsi-ld/v1/entities?type=PatrolRobot",
									"host": [
										"{{context-broker}}"
									],
									"path": [
										"ngsi-ld",
										"v1",
										"entities"
									],
									"query": [
										{
											"key": "type",
											"value": "PatrolRobot"
										}
									]
								}
							},
							"response": []
						},
						{
							"name": "Post Update (HTTP) (Try running this twice)",
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "fiware-service",
										"value": "openiot",
										"type": "text",
										"disabled": true
									},
									{
										"key": "fiware-servicepath",
										"value": "/",
										"type": "text",
										"disabled": true
									},
									{
										"key": "link",
										"value": "<http://localhost:1026/ngsi-ld/v1/jsonldContexts/b813cc1a-ac92-11ee-8358-0242ac120004>;rel=\"http://www.w3.org/ns/json-ld#context\"; type=\"application/ld+json\"",
										"type": "text"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\r\n   \"n\": [\"Dylan\"],\r\n   \"c\": 100\r\n}",
									"options": {
										"raw": {
											"language": "json"
										}
									}
								},
								"url": {
									"raw": "http://{{iot-agent-json}}/iot/json/?i=PatrolRobot:010&k=1234",
									"protocol": "http",
									"host": [
										"{{iot-agent-json}}"
									],
									"path": [
										"iot",
										"json",
										""
									],
									"query": [
										{
											"key": "i",
											"value": "PatrolRobot:010"
										},
										{
											"key": "k",
											"value": "1234"
										}
									]
								}
							},
							"response": []
						},
						{
							"name": "Delete PatrolRobot Entity from IOT agent",
							"request": {
